<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#00b9f5" android:endColor="#00b9f5" android:angle="270" android:centerY="0.75" android:centerColor="#00b9f5"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#00a5e9" android:endColor="#244" android:angle="270" android:centerY="0.75" android:centerColor="#00a5e9"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#00a5e9" android:endColor="#00a5e9" android:angle="270" android:centerY="0.75" android:centerColor="#00a5e9"/>
            </shape>
        </clip>
    </item>
</layer-list>
